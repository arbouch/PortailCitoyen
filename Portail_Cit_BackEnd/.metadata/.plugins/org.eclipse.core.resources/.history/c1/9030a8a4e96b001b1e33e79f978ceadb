package com.example.demo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class PortailCitBackEndApplication {

	public static void main(String[] args) {
		SpringApplication.run(PortailCitBackEndApplication.class, args);
		int min = 50;
	      int max = 100;
	      //Generate random double value from 50 to 100 
	      System.out.println("Random value in double from "+min+" to "+max+ ":");
	      int random_double = (int) (Math.random() * (max - min + 1) + min); 
	      System.out.println("randooooooooooooom"+random_double);
	}

}
