package com.example.demo.UserModel;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class User {
@Id
private int id ;
private String numtel;
private int cin;
private int otp;
public User(int id, String numtel, int cin, int otp) {
	 
	this.id = id;
	this.numtel = numtel;
	this.cin = cin;
	this.otp=otp;
}
public User( ) {
	 
	 
}

public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getNumtel() {
	return numtel;
}
public void setNumtel(String numtel) {
	this.numtel = numtel;
}
public int getCin() {
	return cin;
}
public void setCin(int cin) {
	this.cin = cin;
}
public int getOtp() {
	return otp;
}
public void setOtp(int otp) {
	this.otp = otp;
}


}

